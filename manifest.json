{
    "name": "Timonpp",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": [
          "https://timon.lais.net/*"
        ],
        "js": ["script.js"]
      }
    ],
    "description": "Formats the Timonpp page to be more readable.",
    "permissions": [
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }